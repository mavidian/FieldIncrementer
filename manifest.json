{
    "manifest_version": 2,
    "name": "FieldIncrementer",
    "version": "0.1",
    "description": "Browser extension to increment current field contents",
    "background": {
        "persistent": true,
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "contextMenus",
        "activeTab"
    ],
    "icons": {
        "16": "icons/MavLogo_17_16_16.png",
        "48": "icons/MavLogo_17_48_48.png",
        "128": "icons/MavLogo_17_128_128.png"
    }
}